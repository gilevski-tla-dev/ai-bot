# Telegram Bot Token
# Получите у @BotFather в Telegram
BOT_TOKEN=your_telegram_bot_token_here

# OpenRouter API Key
# Получите на https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# AI Model (по умолчанию бесплатная модель)
